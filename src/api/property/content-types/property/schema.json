{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "area": {
      "type": "biginteger",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "location": {
      "type": "string",
      "required": true
    },
    "agent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::agent.agent",
      "inversedBy": "properties"
    }
  }
}
